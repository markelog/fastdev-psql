{
  "name": "fastdev-psql",
  "version": "1.2.1",
  "description": "Quickly set up psql instance",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js -d dist",
    "watch": "chokidar index.js -c 'npm run build'",
    "lint": "npm run eslint && npm run jscs",
    "jscs": "jscs index.js test",
    "eslint": "eslint index.js test",
    "test": "npm run lint && mocha",
    "coverage": "istanbul cover _mocha -- test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "ci": "npm run lint && npm run coveralls",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markelog/fastdev-psql.git"
  },
  "keywords": [
    "docker",
    "psql",
    "dump",
    "fast",
    "postgres"
  ],
  "author": "Oleg Gaidarenko <markelog@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markelog/fastdev-psql/issues"
  },
  "homepage": "https://github.com/markelog/fastdev-psql",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "chalk": "^1.1.1",
    "cli-spinner": "^0.2.4",
    "dbuilder": "^1.2.0",
    "fs-extra": "^0.30.0",
    "tmp": "0.0.28",
    "vow": "^0.4.12"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.6.5",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.8",
    "eslint": "2.10.2",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "gaze-cli": "^0.2.0",
    "istanbul": "^1.0.0-alpha.2",
    "jscs": "^2.11.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
